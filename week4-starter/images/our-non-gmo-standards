    <script defer>
        document.addEventListener( 'DOMContentLoaded', function ( event ) { // DOMContentLoaded
            // Configure AddThis sharing buttons
            var configureAddThisShareButtons = setInterval ( function () {
                if ( typeof addthis == 'object' &&
                     typeof addPageInteraction == 'function' &&
                     typeof addUDMEvent == 'function' &&
                     typeof adobeTracking == 'boolean' &&
                     typeof isEditMode == 'boolean' &&
                     typeof jQuery == 'function' ) {
                    clearInterval ( configureAddThisShareButtons );

					//#8732 - ADA - Make sure tabindex is 0
					setTimeout(function(){
                        $('a.at-icon-wrapper.at-share-btn').each(function(){
                            $(this).attr('tabindex', '0');
                        });
					}, 2000);

                    // Add Twitter text
                    var twitterText = "Ben &amp Jerry&#039s ice cream is proud that all flavors sold in pints, quarts, mini-cups, and Scoop Shops are made with non-GMO sourced ingredients.";
                    twitterText = twitterText.replace(/[\\]/g, "\\");

                    twitter_options = {
                        passthrough : {
                            twitter : {
                                via : "benandjerrys",
                                text : twitterText,
                                related : ""
                            }
                        }
                    };

                    $.extend(true, addthis_share, twitter_options);

                    // When AddThis buttons are clicked/shared
                    addthis.addEventListener ( 'addthis.menu.share', function ( evt ) {

                        // Default service of share button clicked
                        var serviceOfShareButtonClicked = evt.data.service;

                        // Modify service label
                        switch ( serviceOfShareButtonClicked ) {
                            case 'email':
                                serviceOfShareButtonClicked = 'Email';
                                break;
                            case 'facebook':
                                serviceOfShareButtonClicked = 'Facebook';
                                break;
                            case 'pinterest_share':
                                serviceOfShareButtonClicked = 'Pinterest';
                                break;
                            case 'twitter':
                                serviceOfShareButtonClicked = 'Twitter';
                                break;
                        }

                        // Send event to Google Analytics
                        addUDMEvent ( 'Pages', 'Shared', serviceOfShareButtonClicked );

                        // Send data to Adobe
                        if ( adobeTracking && !isEditMode ){
                            adobeSocialShareContent(serviceOfShareButtonClicked);
                        }
                    });
                }
            }, 1000);
        }); // DOMContentLoaded
    </script>

